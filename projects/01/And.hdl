/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */
//两个数输出一个数，判断两数是否同时等于1
CHIP And
{
        IN a, b;
        OUT out;
		PARTS:
        //Nand(a = a,b = b,out = x);
        //Nand(a = a,b = b, out = y);
		//Nand(a = x,b = y, out = out);
		Nand (a =a, b = b, out = x);
		Not (in = x, out = out);
}